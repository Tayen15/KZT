{
     "name": "bytebot",
     "version": "1.5.0",
     "description": "A powerful Discord bot featuring music, moderation, information commands, and fun utilities.",
     "main": "index.js",
     "scripts": {
          "test": "node .",
          "start": "node bot.js",
          "dev": "cross-env NODE_ENV=development node bot.js",
          "dev:bot": "cross-env NODE_ENV=development node index.js",
          "dev:web": "cross-env NODE_ENV=development node server.js",
          "prod": "cross-env NODE_ENV=production node bot.js",
          "start:bot": "node index.js",
          "start:web": "node server.js",
          "build:css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/output.css --minify",
          "watch:css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/output.css --watch",
          "postinstall": "npx prisma generate",
          "heroku-postbuild": "npx prisma db push && npx prisma generate"
     },
     "keywords": [
          "Discord",
          "Bot",
          "Music",
          "Moderation",
          "Information"
     ],
     "author": "Kyonezet",
     "license": "ISC",
     "engines": {
          "node": "20.x",
          "npm": ">=9.x"
     },
     "dependencies": {
          "@discordjs/opus": "^0.9.0",
          "@discordjs/voice": "^0.18.0",
          "@prisma/client": "^6.18.0",
          "axios": "^1.11.0",
          "bcrypt": "^6.0.0",
          "chalk": "^5.6.0",
          "connect-mongo": "^5.1.0",
          "cpu-stat": "^2.0.1",
          "discord.js": "^14.21.0",
          "dotenv": "^17.2.1",
          "ejs": "^3.1.10",
          "express": "^5.1.0",
          "express-session": "^1.18.2",
          "genius-lyrics": "^4.4.7",
          "moment": "^2.29.4",
          "moment-timezone": "^0.6.0",
          "mongoose": "^8.7.0",
          "ms": "^2.1.3",
          "namaz": "^4.2.0",
          "node-fetch": "^3.3.2",
          "opusscript": "0.0.8",
          "os": "^0.1.2",
          "passport": "^0.7.0",
          "passport-discord": "^0.1.4",
          "path": "^0.12.7",
          "play-dl": "^1.9.7",
          "prism-media": "^1.3.5",
          "prisma": "^6.18.0",
          "undici": "^7.13.0"
     },
     "devDependencies": {
          "autoprefixer": "^10.4.21",
          "cross-env": "^7.0.3",
          "postcss": "^8.5.6",
          "tailwindcss": "^4.1.16"
     }
}
